def shout(func):
    def wrapper(*args, **kwargs):
        result = "Welcome: " + func(*args, **kwargs)
        return result
    return wrapper

def whisper(func):
    def wrapper(*args, **kwargs):
        return func(*args, **kwargs).lower()
    return wrapper
    


@whisper
def hello(name: str) -> str:
    return str(name)




print(hello('**'))
